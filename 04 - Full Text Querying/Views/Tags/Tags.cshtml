@model IEnumerable<_04___Full_Text_Querying.Controllers.Posts_ByTag.ReduceResult>
           

<div class="row">
    <div class="span8 offset2">
        @foreach (var tag in Model)
        {
            <div class="row">
                <div class="span8">
                    @tag.Count @Html.ActionLink(tag.Tag, "Tagged", "Tags", new { tag = tag.Tag }, null) 
                </div>
            </div>
        }
    </div>
</div>